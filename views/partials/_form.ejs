 <!-- form handling -->
 <form action="/subjects/createSubject" enctype="multipart/form-data"  method="POST" id="add_subject">
    <div class="new_subject">
        <div class="form-group">
            <label for="imgUpload" class="form-label">Image</label>
            <input type="file" class="form-control" id="imageUpload" name="file">
          </div>
        <div class="form-group">
            <label for="subjFullName" class="text-light">Full Name</label>
            <input type="text" name="subjFullName" value="" placeholder="Mark Stoenis">
        </div>
        <div class="form-group">
            <label for="subjStudyNum" class="text-light">Subject Number</label>
            <input type="text" name="subjStudyNum" value="" placeholder="2656566">
        </div>
        <div class="form-group">
            <label for="dob" class="text-light">Date of Birth</label>
            <input type="date" id="dob" value="" name="dob" placeholder="01-Jan-2000"/> 
        </div>

        <!-- <div class="form-group">
            <label for="age" class="form-label">Age</label>
            <input type="file" class="form-control" name="age">
          </div> -->

          <div class="form-group">
            <label for="telephone" class="form-label">Telephone Number</label>
            <input type="text" class="form-control"  name="telephone">
          </div>
          <div class="form-group">
            <label for="email" class="form-label">Email</label>
            <input type="email" class="form-control"  name="email">
          </div>

          <div class="form-group">
            <label for="subjHomeAdress" class="form-label">Home Address</label>
            <input type="text" class="form-control"  name="subjHomeAdress">
          </div>
          
         <div class="form-check">
            <label for="gender" class="text-light">Gender</label>
            <div >
                <input type="radio" id="radio-2" name="gender" value="Male" >
                <label for="radio-2" class="radio-label">Male</label>
            </div>
            <div >
                <input type="radio" id="radio-3" name="gender" value="Female" >
                <label for="radio-3" class="radio-label">Female</label>
            </div>
        </div>

        <div class="form-check">
            <label for="gender" >Status</label>
            <div >
                <input type="radio" id="radio-4" name="status" value="Enrolled" >
                <label for="radio-4" >Enrolled</label>
            </div>
            <div >
                <input type="radio" id="radio-4" name="status" value="Screening" checked="checked" >
                <label for="radio-4" class="radio-label">Screening</label>
            </div>
            <div >
                <input type="radio" id="radio-4" name="status" value="Completed" >
                <label for="radio-4" class="radio-label">Completed</label>
            </div>
            <div >
                <input type="radio" id="radio-5" name="status" value="Screen Fail" >
                <label for="radio-5" class="radio-label">Screen Fail</label>
            </div>
            <div >
                <input type="radio" id="radio-5" name="status" value="Lost to Follow Up" >
                <label for="radio-5" class="radio-label"> Lost to Follow Up</label>
            </div>
        </div>
 
         <div class="form-group">
            <button type="submit" class="btn" value="Upload">Submit</button>
        </div> 

    </div>
</form>